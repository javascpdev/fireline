{
  "name": "@quiver/fireline",
  "version": "1.0.6",
  "module": "cjs/index.js",
  "main": "functions/exports.js",
  "author": "Chris Esplin <chris@christopheresplin.com> (https://www.chrisesplin.com/",
  "repository": "https://github.com/deltaepsilon/fireline.git",
  "homepage": "https://fireline.chrisesplin.com/",
  "license": "MIT",
  "private": false,
  "keywords": [
    "firebase",
    "firestore",
    "stripe"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.7.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "firebase-tools": "^8.4.2",
    "front-matter": "^4.0.2",
    "lit-html": "^1.2.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "stripe": "^8.62.0"
  },
  "scripts": {
    "build": "cd content && yarn webpack && eleventy",
    "ci:build": "yarn build",
    "ci:config": "yarn deploy:functions:config",
    "ci:deploy": "yarn ci:service-account && yarn deploy",
    "ci:service-account": "echo $SERVICE_ACCOUNT_BASE64 | base64 -d > $GOOGLE_APPLICATION_CREDENTIALS",
    "ci:test": "yarn ci:service-account && yarn test",
    "dev": "yarn start",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:firestore": "firebase deploy --only firestore --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:functions": "firebase deploy --only functions --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:functions:config": "sh bin/firebase-config.sh",
    "deploy:hosting": "firebase deploy --only hosting --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "serve:hosting": "firebase serve --only hosting --port 4000 --host 0.0.0.0",
    "serve:https": "nodemon functions/serve-https",
    "start": "cd content && eleventy --serve --port=4000 & yarn webpack:watch",
    "test": "cd functions && yarn test",
    "test:watch": "cd functions && yarn test:watch",
    "webpack": "webpack --config webpack.config.js --mode production",
    "webpack:watch": "yarn webpack --watch",
    "_": ""
  }
}
