{
  "name": "Fireline",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Chris Esplin <chris@christopheresplin.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.7.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "firebase-tools": "^8.4.2",
    "front-matter": "^4.0.2",
    "lit-html": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "cd content && eleventy & yarn webpack",
    "dev": "yarn start",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:functions": "firebase deploy --only functions --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:functions:config": "firebase functions:config:set stripe.pk=$STRIPE_PK stripe.sk=$STRIPE_SK --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "deploy:hosting": "firebase deploy --only hosting --token $FIREBASE_TOKEN --project=$FIREBASE_PROJECT",
    "serve:hosting": "firebase serve --only hosting --port 4000 --host 0.0.0.0",
    "start": "cd content && eleventy --serve --port=4000 & yarn webpack:watch",
    "webpack": "webpack --config webpack.config.js --mode production",
    "webpack:watch": "yarn webpack --watch",
    "_": ""
  }
}
